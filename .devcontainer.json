{
  "name": "docker-nginx-mysql-rails-vue-backend",
	"dockerComposeFile": "../docker-compose.yml",
	"service": "backend",
	"workspaceFolder": "/backend",
	"features": {
    "ghcr.io/devcontainers/features/common-utils:2": {},
		"ghcr.io/devcontainers/features/git:1": {}
	},
  "customizations": {
		"vscode": {
			"extensions": [
				"eamodio.gitlens",
				"shopify.ruby-lsp"
			]
		}
	},
	"shutdownAction": "none"
}